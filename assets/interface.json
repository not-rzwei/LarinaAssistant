                {
                    "name": "Disable",
                    "pipeline_override": {
                        "Mall_DailyLimited_Buy_SRankContract": {
                            "enabled": false
                        }
                    }
                }
            ],
            "default_case": "Disable"
        },
        "Vanguard Squad": {
            "cases": [
                {
                    "name": "01",
                    "pipeline_override": {
                        "Stage_Squad_Select": {
                            "recognition": {
                                "param": {
                                    "expected": [
                                        "01"
                                    ]
                                }
                            }
                        }
                    }
                },
                {
                    "name": "02",
                    "pipeline_override": {
                        "Stage_Squad_Select": {
                            "recognition": {
                                "param": {
                                    "expected": [
                                        "02"
                                    ]
                                }
                            }
                        }
                    }
                },
                {
                    "name": "03",
                    "pipeline_override": {
