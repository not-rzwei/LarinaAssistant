{
    "name": "Larina Assistant",
    "url": "https://github.com/not-rzwei/MaaHW",
    "mfa_max_version": "v1.6.5",
    "custom_title": "Larina Assistant",
    "controller": [
        {
            "name": "ADB Default Method",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "Onestore English",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "{PROJECT_DIR}/python/python.exe",
        "child_args": [
            "-u",
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "Start",
            "entry": "Startup_Entry",
            "check": true
        },
        {
            "name": "Devotion Reward",
            "entry": "Lobby_Tribute_Entry",
            "check": true
        },
        {
            "name": "Fairy Mall",
            "entry": "Mall_Entry",
            "check": true
        }
    ],
    "option": {}
}