{
    "name": "Larina Assistant",
    "url": "https://github.com/not-rzwei/LarinaAssistant",
    "mfa_max_version": "v1.6.5",
    "custom_title": "Larina Assistant",
    "controller": [
        {
            "name": "ADB Default Method",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "Onestore English",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "{PROJECT_DIR}/python/python.exe",
        "child_args": [
            "-u",
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "Startup",
            "entry": "Startup_Entry"
        },
        {
            "name": "Devotion Reward",
            "entry": "Lobby_Tribute_Entry",
            "option": [
                "Devotion Reward Threshold"
            ]
        },
        {
            "name": "Special Ops Reward",
            "entry": "Lobby_SpecialOps_Entry",
            "option": [
                "Special Ops Reward Threshold"
            ]
        },
        {
            "name": "Mailbox",
            "entry": "Lobby_Mail_Entry"
        },
        {
            "name": "Fairy Mall",
            "entry": "Mall_Entry",
            "option": [
                "Gift Purchase",
                "B Rank Contract",
                "A Rank Contract",
                "S Rank Contract"
            ]
        },
        {
            "name": "Daily Wishes",
            "entry": "Stage_Wish_Entry",
            "option": [
                "Vanguard Squad",
                "Ticket 1",
                "Ticket 2",
                "Ticket 3"
            ]
        },
        {
            "name": "Daily Bounties",
            "entry": "Stage_Bounty_Entry",
            "option": [
                "Vanguard Squad",
                "Bounty Selection"
            ]
        },
        {
            "name": "Weekly Rifts",
            "entry": "Stage_Rift_Entry",
            "option": [
                "Vanguard Squad"
            ]
        },
        {
            "name": "Daily Mission",
            "entry": "Lobby_Mission_Entry"
        }
    ],
    "option": {
        "Gift Purchase": {
            "cases": [
                {
                    "name": "Enable",
                    "pipeline_override": {
                        "Mall_DailyLimited_Buy_GiftBox": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "Disable",
                    "pipeline_override": {
                        "Mall_DailyLimited_Buy_GiftBox": {
                            "enabled": false
                        }
                    }
                }
            ],
            "default_case": "Disable"
        },
        "B Rank Contract": {
            "cases": [
                {
                    "name": "Enable",
                    "pipeline_override": {
                        "Mall_DailyLimited_Buy_BRankContract": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "Disable",
                    "pipeline_override": {
                        "Mall_DailyLimited_Buy_BRankContract": {
                            "enabled": false
                        }
                    }
                }
            ],
            "default_case": "Disable"
        },
        "A Rank Contract": {
            "cases": [
                {
                    "name": "Enable",
                    "pipeline_override": {
                        "Mall_DailyLimited_Buy_ARankContract": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "Disable",
                    "pipeline_override": {
                        "Mall_DailyLimited_Buy_ARankContract": {
                            "enabled": false
                        }
                    }
                }
            ],
            "default_case": "Disable"
        },
        "S Rank Contract": {
            "cases": [
                {
                    "name": "Enable",
                    "pipeline_override": {
                        "Mall_DailyLimited_Buy_SRankContract": {
