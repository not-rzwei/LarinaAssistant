        "Devotion Reward Threshold": {
            "default_case": "50%",
            "cases": [
                {
                    "name": "50%",
                    "pipeline_override": {
                        "Lobby_Tribute_Enter_Any": {
                            "enabled": false
                        },
                        "Lobby_Tribute_Enter_Percentage": {
                            "enabled": true,
                            "recognition": {
                                "param": {
                                    "expected": [
                                        "50%"
                                    ]
                                }
                            }
                        }
                    }
                },
                {
                    "name": "Any%",
                    "pipeline_override": {
                        "Lobby_Tribute_Enter_Any": {
                            "enabled": true
                        },
                        "Lobby_Tribute_Enter_Percentage": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "Special Ops Reward Threshold": {
            "default_case": "50%",
            "cases": [
                {
                    "name": "50%",
                    "pipeline_override": {
                        "Lobby_SpecialOps_Enter_Any": {
                            "enabled": false
                        },
                        "Lobby_SpecialOps_Enter_Percentage": {
                            "enabled": true,
                            "recognition": {
                                "param": {
                                    "expected": [
                                        "[5-9][0-9]"
                                    ]
                                }
                            }
                        }
                    }
                },
                {
                    "name": "Any%",
                    "pipeline_override": {
